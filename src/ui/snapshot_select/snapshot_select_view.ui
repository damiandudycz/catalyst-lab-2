<?xml version="1.0" encoding="UTF-8"?>
<interface>

  <!-- Libraries -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="Adw" version="1.0"/>

  <!-- Template -->
  <template class="SnapshotSelectionView" parent="GtkBox">
    <child>
      <object class="AdwPreferencesGroup" id="snapshots_list">
        <property name="title">Select Portage snapshot</property>
        <property name="description">Select Portage snapshot to use.</property>
      </object>
    </child>

  </template>
</interface>

